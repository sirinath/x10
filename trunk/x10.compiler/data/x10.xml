<?xml version='1.0' ?>
<x10_pem_events>
<event name="X10::Event::RemoteCall" description="Record a remote call">
   <layerId value="X10" />
   <classId value="Event" />
   <specifier value="RemoteCall" />
   <fields>
      <field name="systemTime" type="uint64" description="system time in milliseconds" />
      <field name="pid"        type="uint32" description="place identifier" />
      <field name="reserved"   type="uint32" description="reserved for 64-bit alignment (0)" />
   </fields>
   <k42Format value='pid=%1[%d] time=%0[%lld]' />
</event>


