<event name="X10::Sampler::Load" description="Record load data for all places">
   <layerId value="X10" />
   <classId value="Sampler" />
   <specifier value="Load" />
   <fields>
      <field name="systemTime" type="uint64" description="system time in milliseconds" />
      <field name="numberOfPlaces" type="uint32" description="number of places" />
      <field name="reserved"   type="uint32" description="reserved for 64-bit alignment (0)" />
      <field name="places" type="list" size="numberOfPlaces" eltType="uint32" description="list of place IDs" />
      <field name="load" type="list" size="numberOfPlaces" eltType="uint32" description="load values" />
	
   </fields>
   <k42Format value='time=%0[%lld] number=%1[%d]' />
</event>

