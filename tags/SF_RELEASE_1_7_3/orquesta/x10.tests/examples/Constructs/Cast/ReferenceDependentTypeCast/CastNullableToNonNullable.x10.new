public class CastNullableToNonNullable extends x10Test {

	public def run(): boolean = {
		var var: X10DepTypeClassTwo = new X10DepTypeClassTwo(1,2);
		
		// no dep type hence regular java cast apply which allows null value casting
		var nullableVarCasted1: nullable<X10DepTypeClassTwo> = (nullable<X10DepTypeClassTwo>) var;

		// to nullable but expr != null
		var nullableVarCasted2: nullable<X10DepTypeClassTwo> = (nullable<X10DepTypeClassTwo>) new X10DepTypeClassTwo(1,2) ;

		return true;
	}

	public static def main(var args: Rail[String]): void = {
		new CastNullableToNonNullable().execute();
	}
}
