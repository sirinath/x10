## Makefile for X10Lib Benchmarks.

MAKE = make
SUBDIRS = rand
RM = rm -f

all:
	(cd rand; $(MAKE) $@)

install:
	(cd rand; $(MAKE) $@)

clean:
	(cd rand; $(MAKE) $@)
