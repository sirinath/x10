## Makefile for X10Lib RandomAccess Benchmark.

MAKE = make
SUBDIRS = c cpp
RM = rm -f

all:
	(cd c; $(MAKE) $@)
	(cd cpp; $(MAKE) $@)

install:
	(cd c; $(MAKE) $@)
	(cd cpp; $(MAKE) $@)

clean:
	(cd c; $(MAKE) $@)
	(cd cpp; $(MAKE) $@)
