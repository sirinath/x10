package x10.array;

/**
 * This class represents a one-dimensional point.
 * Implementation notes: See RectRegion1.
 *
 * @author jgalens
 */

public final value class Point1(x1: Int) extends Point{self.rank == 1} {
	
    def this(x1: Int): Point1{self.x1 == x1} {
    	super([x1]);
    	property(x1);
    }
	
}