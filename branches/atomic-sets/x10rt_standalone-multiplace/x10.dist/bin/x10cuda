#!/bin/bash

UNAME=`uname -s`

prog=$(readlink $0 2>&1)
[ $? -eq 127 -o "$prog" = "" ] && prog="$0"
export X10_DIST="$(cd "$(dirname "$prog")/.." && pwd)"
if [[ "$UNAME" = CYGWIN* ]]; then X10_DIST="$(cygpath -aw "$X10_DIST")"; fi

exec "${X10_DIST}/bin/x10c++" -J-ea -extclass x10cuda.ExtensionInfo "$@"

