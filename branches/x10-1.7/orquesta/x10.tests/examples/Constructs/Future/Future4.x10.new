import harness.x10Test;;

/**
 * A future test.
 */
public class Future4 extends x10Test {
	public def run(): boolean = {
		var x: future<int> = future { 46 };
		return (x.force()+1) == 47;
	}

	public static def main(var args: Rail[String]): void = {
		new Future4().execute();
	}
}
